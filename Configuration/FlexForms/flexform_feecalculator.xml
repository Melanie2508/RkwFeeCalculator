<T3DataStructure>
    <sheets>
        <general>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:rkw_feecalculator/Resources/Private/Language/locallang_db.xlf:flexform.feecalculator.sheetTitle</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.name>
                        <label>LLL:EXT:rkw_feecalculator/Resources/Private/Language/locallang_db.xlf:tx_rkwfeecalculator_domain_model_calculator.name</label>
                        <config>
                            <type>input</type>
                            <size>20</size>
                            <eval>trim</eval>
                        </config>
                    </settings.name>
                    <settings.assignedPrograms>
                        <TCEforms>
                            <label>LLL:EXT:rkw_feecalculator/Resources/Private/Language/locallang_db.xlf:tx_rkwfeecalculator_domain_model_calculator.assigned_programs</label>
                            <config>
                                <size>5</size>
                                <maxitems>5</maxitems>
                                <minitems>0</minitems>
                                <type>select</type>
                                <foreign_table>tx_rkwfeecalculator_domain_model_program</foreign_table>
                                <foreign_table_where>
                                    AND tx_rkwfeecalculator_domain_model_program.deleted = 0 AND tx_rkwfeecalculator_domain_model_program.hidden = 0 ORDER BY tx_rkwfeecalculator_domain_model_program.name
                                </foreign_table_where>
                            </config>
                        </TCEforms>
                    </settings.assignedPrograms>
                    <settings.days>
                        <label>LLL:EXT:rkw_feecalculator/Resources/Private/Language/locallang_db.xlf:tx_rkwfeecalculator_domain_model_calculator.days</label>
                        <config>
                            <type>input</type>
                            <size>20</size>
                            <eval>trim</eval>
                        </config>
                    </settings.days>
                    <settings.consultant_fee_per_day>
                        <label>LLL:EXT:rkw_feecalculator/Resources/Private/Language/locallang_db.xlf:tx_rkwfeecalculator_domain_model_calculator.consultant_fee_per_day</label>
                        <config>
                            <type>input</type>
                            <size>20</size>
                            <eval>trim</eval>
                        </config>
                    </settings.consultant_fee_per_day>
                </el>
            </ROOT>
        </general>
    </sheets>
</T3DataStructure>
